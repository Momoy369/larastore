{"ast":null,"code":"import Category from '@/components/Category';\nimport Slider from '@/components/Slider';\nimport { computed, onMounted } from 'vue';\nimport { useStore } from 'vuex';\nexport default {\n  components: {\n    Category,\n    Slider\n  },\n  setup() {\n    // Store vuex\n    const store = useStore();\n\n    // onMounted akan menjalankan action \"getProducts\" di module \"product\"\n    onMounted(() => {\n      store.dispatch('product/getProducts');\n    });\n\n    // Computed properti digunakan untuk get data product dari state di module product\n    const products = computed(() => {\n      return store.state.product.products;\n    });\n    return {\n      store,\n      products\n    };\n  }\n};","map":{"version":3,"mappings":"AAqDA,OAAOA,QAAO,MAAO,uBAAsB;AAC3C,OAAOC,MAAK,MAAO,qBAAoB;AACvC,SAASC,QAAQ,EAAEC,SAAQ,QAAS,KAAI;AACxC,SAASC,QAAO,QAAS,MAAK;AAE9B,eAAe;EAEXC,UAAU,EAAE;IACRL,QAAQ;IACRC;EACJ,CAAC;EAEDK,KAAK,GAAG;IAEJ;IACA,MAAMC,KAAI,GAAIH,QAAQ,EAAC;;IAEvB;IACAD,SAAS,CAAC,MAAM;MACZI,KAAK,CAACC,QAAQ,CAAC,qBAAqB;IACxC,CAAC;;IAED;IACA,MAAMC,QAAO,GAAIP,QAAQ,CAAC,MAAM;MAC5B,OAAOK,KAAK,CAACG,KAAK,CAACC,OAAO,CAACF,QAAO;IACtC,CAAC;IAED,OAAO;MACHF,KAAK;MACLE;IACJ;EACJ;AACJ","names":["Category","Slider","computed","onMounted","useStore","components","setup","store","dispatch","products","state","product"],"sourceRoot":"","sources":["D:\\xampp\\htdocs\\larastore\\frontend-larastore\\src\\views\\home\\Index.vue"],"sourcesContent":["<template>\r\n    <div class=\"container-fluid mt-3\">\r\n        <div class=\"row\">\r\n            <div class=\"col-md-3 mb-4\">\r\n                <Category />\r\n            </div>\r\n            <div class=\"col-md-9 mb-4\">\r\n                <Slider />\r\n            </div>\r\n        </div>\r\n    </div>\r\n    \r\n    <div class=\"container-fluid mb-5 mt-4\">\r\n        <div class=\"row\">\r\n            <div class=\"col-md-12\">\r\n                <h4 class=\"font-weight-bold\">\r\n                    <i class=\"fa fa-shopping-bag\"></i>\r\n                    NEW PRODUCTS\r\n                </h4>\r\n                <hr style=\"border-top: 5px solid rgb(154 155 156); border-radius: .5rem;\">\r\n            </div>\r\n        </div>\r\n        <div class=\"row\">\r\n            <div v-for=\"product in products\" :key=\"product.id\" class=\"col-md-3 col-mb-6\">\r\n                <div class=\"card h-100 border-0 shadow rounded-md\">\r\n                    <div class=\"card-img\">\r\n                        <img :src=\"product.image\" class=\"1-100\" style=\"height: 15rem; object-fit: cover; border-top-left-radius: .25rem; border-top-right-radius: .25rem;\">\r\n                    </div>\r\n                    <div class=\"card-body\">\r\n                        <router-link :to=\"{name: 'detail_product', params:{slug: product.slug}}\" class=\"card-title font-weight-bold\" style=\"font-size: 20px;\">\r\n                            {{ product.title }}\r\n                        </router-link>\r\n                        <div class=\"dicount mt-2\" style=\"color: #999;\">\r\n                            <s>Rp. {{ moneyFormat(product.price) }}</s>\r\n                            <span class=\"badge badge-pill badge-success text-white\">\r\n                                DISCOUNT {{ product.discount }} %\r\n                            </span>\r\n                        </div>\r\n                        <div class=\"price font-weight-bold mt-3\" style=\"color: #47b04b; font-size: 20px;\">\r\n                            Rp. {{ moneyFormat(calculatedDiscount(product)) }}\r\n                        </div>\r\n                        <router-link :to=\"{name: 'detail_product', params:{slug: product.slug}}\" class=\"btn btn-primary btn-md mt-3 btn-block shadow-md\">\r\n                            VIEW PRODUCT\r\n                        </router-link>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n\r\nimport Category from '@/components/Category'\r\nimport Slider from '@/components/Slider'\r\nimport { computed, onMounted } from 'vue'\r\nimport { useStore } from 'vuex'\r\n\r\nexport default {\r\n\r\n    components: {\r\n        Category,\r\n        Slider\r\n    },\r\n\r\n    setup() {\r\n\r\n        // Store vuex\r\n        const store = useStore()\r\n\r\n        // onMounted akan menjalankan action \"getProducts\" di module \"product\"\r\n        onMounted(() => {\r\n            store.dispatch('product/getProducts')\r\n        })\r\n\r\n        // Computed properti digunakan untuk get data product dari state di module product\r\n        const products = computed(() => {\r\n            return store.state.product.products\r\n        })\r\n\r\n        return {\r\n            store,\r\n            products\r\n        }\r\n    }\r\n}\r\n\r\n</script>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}