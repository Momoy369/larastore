{"ast":null,"code":"//import customer menu component\nimport CustomerMenu from '@/components/CustomerMenu';\nimport { computed, onMounted } from 'vue';\nimport { useStore } from 'vuex';\nexport default {\n  name: 'OrderIndexComponent',\n  components: {\n    //customer menu component\n    CustomerMenu\n  },\n  setup() {\n    //store vuex\n    const store = useStore();\n\n    //mounted\n    onMounted(() => {\n      //panggil action \"getOrder\" di module \"order\" vuex\n      return store.dispatch('order/getOrder');\n    });\n\n    //computed\n    const orders = computed(() => {\n      //panggil getter dengan nama \"getOrder\" di module \"order\" vuex \n      return store.getters['order/getOrder'];\n    });\n\n    //return a state and function\n    return {\n      store,\n      orders\n    };\n  }\n};","map":{"version":3,"mappings":"AA0CI;AACA,OAAOA,YAAW,MAAO,2BAA0B;AACnD,SAASC,QAAQ,EAAEC,SAAQ,QAAS,KAAI;AACxC,SAASC,QAAO,QAAS,MAAK;AAE9B,eAAe;EAEXC,IAAI,EAAE,qBAAqB;EAE3BC,UAAU,EAAE;IACR;IACAL;EACJ,CAAC;EAEDM,KAAK,GAAG;IAEJ;IACA,MAAMC,KAAI,GAAIJ,QAAQ,EAAC;;IAEvB;IACAD,SAAS,CAAC,MAAM;MAEZ;MACA,OAAOK,KAAK,CAACC,QAAQ,CAAC,gBAAgB;IAE1C,CAAC;;IAED;IACA,MAAMC,MAAK,GAAIR,QAAQ,CAAC,MAAM;MAE1B;MACA,OAAOM,KAAK,CAACG,OAAO,CAAC,gBAAgB;IAEzC,CAAC;;IAED;IACA,OAAO;MACHH,KAAK;MACLE;IACJ;EAEJ;AAEJ","names":["CustomerMenu","computed","onMounted","useStore","name","components","setup","store","dispatch","orders","getters"],"sourceRoot":"","sources":["D:\\xampp\\htdocs\\larastore\\frontend-larastore\\src\\views\\order\\Index.vue"],"sourcesContent":["<template>\r\n    <div class=\"container-fluid mb-5 mt-4\">\r\n        <div class=\"row\">\r\n            <div class=\"col-md-3 mb-4\">\r\n                <CustomerMenu />\r\n            </div>\r\n            <div class=\"col-md-9 mb-4\">\r\n                <div class=\"card border-0 rounded shadow\">\r\n                    <div class=\"card-body\">\r\n                        <h5 class=\"font-weight-bold\"> <i class=\"fas fa-shopping-cart\"></i> MY ORDER</h5>\r\n                        <hr>\r\n                        <table class=\"table table-striped table-bordered\">\r\n                            <thead class=\"thead-dark\">\r\n                                <tr>\r\n                                    <th scope=\"col\">INVOICE</th>\r\n                                    <th scope=\"col\">FULL NAME</th>\r\n                                    <th scope=\"col\">SHIPPING</th>\r\n                                    <th scope=\"col\">GRAND TOTAL</th>\r\n                                    <th scope=\"col\">OPTIONS</th>\r\n                                </tr>\r\n                            </thead>\r\n                            <tbody>\r\n                                <tr v-for=\"order in orders\" :key=\"order.id\">\r\n                                    <th>{{ order.invoice }}</th>\r\n                                    <td>{{ order.name }}</td>\r\n                                    <td>{{ order.courier.toUpperCase() }} | {{ order.service }} | Rp. {{ moneyFormat(order.cost_courier) }}</td>\r\n                                    <td>Rp. {{ moneyFormat(order.grand_total) }}</td>\r\n                                    <td class=\"text-center\">\r\n                                        <router-link :to=\"{name: 'detail_order', params:{snap_token: order.snap_token}}\" class=\"btn btn-sm btn-primary\">DETAIL</router-link>\r\n                                    </td>\r\n                                </tr>\r\n                                \r\n                            </tbody>\r\n                        </table>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    //import customer menu component\r\n    import CustomerMenu from '@/components/CustomerMenu'\r\n    import { computed, onMounted } from 'vue'\r\n    import { useStore } from 'vuex'\r\n\r\n    export default {\r\n\r\n        name: 'OrderIndexComponent',\r\n\r\n        components: {\r\n            //customer menu component\r\n            CustomerMenu\r\n        },\r\n\r\n        setup() {\r\n\r\n            //store vuex\r\n            const store = useStore()\r\n\r\n            //mounted\r\n            onMounted(() => {\r\n\r\n                //panggil action \"getOrder\" di module \"order\" vuex\r\n                return store.dispatch('order/getOrder')\r\n\r\n            })\r\n\r\n            //computed\r\n            const orders = computed(() => {\r\n\r\n                //panggil getter dengan nama \"getOrder\" di module \"order\" vuex \r\n                return store.getters['order/getOrder']\r\n\r\n            })\r\n\r\n            //return a state and function\r\n            return {\r\n                store,\r\n                orders\r\n            }\r\n\r\n        }\r\n\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n    .table .thead-dark th {\r\n        color: #fff;\r\n        background-color: #6777ef;\r\n        border-color: #6777ef;\r\n    }\r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}