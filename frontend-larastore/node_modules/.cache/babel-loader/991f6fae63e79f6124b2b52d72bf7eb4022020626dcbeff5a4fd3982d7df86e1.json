{"ast":null,"code":"export default {\n  name: 'LoginComponent'\n};","map":{"version":3,"mappings":"AAwCI,eAAe;EACXA,IAAI,EAAE;AACV","names":["name"],"sourceRoot":"","sources":["D:\\xampp\\htdocs\\larastore\\frontend-larastore\\src\\views\\auth\\Login.vue"],"sourcesContent":["<template>\r\n    <div class=\"container-fluid mb-5 mt-5\">\r\n        <div class=\"row justify-content-center\">\r\n            <div class=\"col-md-4\">\r\n                <div v-if=\"validation.message\" class=\"mt-2 alert alert-danger\">\r\n                    {{ validation.message }}\r\n                </div>\r\n                <div class=\"card border-0 shadow rounded\">\r\n                    <div class=\"card-body\">\r\n                        <h4>LOGIN</h4>\r\n                        <hr>\r\n                        <form @submit-prevent=\"login\">\r\n                            <div class=\"form-group\">\r\n                                <label>Email Address</label>\r\n                                <input type=\"email\" v-model=\"user.email\" class=\"form-control\" placeholder=\"Email Address\">\r\n                            </div>\r\n                            <div v-if=\"validation.email\" class=\"mt-2 alert alert-danger\">\r\n                                {{ validation.email[0] }}\r\n                            </div>\r\n                            <div class=\"form-group\">\r\n                                <label>Password</label>\r\n                                <input type=\"password\" v-modal=\"user.password\" class=\"form-control\" placeholder=\"Enter password\">\r\n                            </div>\r\n                            <div v-if=\"validation.password\" class=\"alert alert-danger\">\r\n                                {{ validation.passwords[0] }}\r\n                            </div>\r\n                            <div class=\"form-group form-check\">\r\n                                <input type=\"checkbox\" class=\"form-check-input\" id=\"exampleCheck1\">\