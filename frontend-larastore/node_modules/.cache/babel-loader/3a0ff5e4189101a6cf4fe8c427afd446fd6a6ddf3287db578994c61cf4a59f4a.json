{"ast":null,"code":"import { computed, onMounted } from 'vue';\nimport { useStore } from 'vuex';\nimport { useRoute, useRouter } from 'vue-router';\nexport default {\n  name: 'ShowProductComponent',\n  setup() {\n    // Vue route\n    const route = useRoute();\n\n    // Vue router\n    const router = useRouter();\n\n    // Store vuex\n    const store = useStore();\n\n    // onMounted akan menjalankan action \"getDetailProduct\" di module \"product\" Vuex\n    onMounted(() => {\n      store.dispatch('product');\n    });\n  }\n};","map":{"version":3,"mappings":"AA2EI,SAAQA,QAAQ,EAAEC,SAAQ,QAAS,KAAI;AACvC,SAASC,QAAO,QAAS,MAAK;AAC9B,SAASC,QAAQ,EAAEC,SAAQ,QAAS,YAAW;AAE/C,eAAe;EACXC,IAAI,EAAE,sBAAsB;EAE5BC,KAAK,GAAG;IAEJ;IACA,MAAMC,KAAI,GAAIJ,QAAQ,EAAC;;IAEvB;IACA,MAAMK,MAAK,GAAIJ,SAAS,EAAC;;IAEzB;IACA,MAAMK,KAAI,GAAIP,QAAQ,EAAC;;IAEvB;IACAD,SAAS,CAAC,MAAM;MACZQ,KAAK,CAACC,QAAQ,CAAC,SAAS;IAC5B,CAAC;EACL;AACJ","names":["computed","onMounted","useStore","useRoute","useRouter","name","setup","route","router","store","dispatch"],"sourceRoot":"","sources":["D:\\xampp\\htdocs\\larastore\\frontend-larastore\\src\\views\\product\\Show.vue"],"sourcesContent":["<template>\r\n    <div class=\"container mt-5 mb-5\">\r\n        <div class=\"row\">\r\n            <div class=\"col-md-4\">\r\n                <div class=\"card border-0 shadow rounded\">\r\n                    <div class=\"card-body p-2\">\r\n                        <img :src=\"product.image\" class=\"w-100 border\">\r\n                    </div>\r\n                </div>\r\n            </div>\r\n            <div class=\"col-md-8\">\r\n                <div class=\"card border-0 rounded shadow\">\r\n                    <div class=\"card-body\">\r\n                        <label class=\"font-weight-bold\" style=\"font-size: 20px;\">\r\n                            {{ product.title }}\r\n                        </label>\r\n                        <hr>\r\n                        <div class=\"price-product\" id=\"price-product\" style=\"font-size: 1.35rem;\">\r\n                            <span class=\"font-weight-bold mr-4\" style=\"color: green;\">\r\n                                Rp. {{ calculateDiscount(product) }}\r\n                            </span>\r\n                            <s class=\"font-weight-bold\" style=\"text-decoration-color: red;\">\r\n                                Rp. {{ product.price }}\r\n                            </s>\r\n                        </div>\r\n                        <table class=\"table table-borderless mt-3\">\r\n                            <tbody>\r\n                                <tr>\r\n                                    <td class=\"font-weight-bold\">DISCOUNT</td>\r\n                                    <td>:</td>\r\n                                    <td>\r\n                                        <button class=\"btn btn-sm\" style=\"color: #ff2f00;border-color: #ff2f00;\">\r\n                                            DISCOUNT {{ product.discount }} %\r\n                                        </button>\r\n                                    </td>\r\n                                </tr>\r\n                                <tr>\r\n                                    <td class=\"font-weight-bold\">\r\n                                        WEIGHT\r\n                                    </td>\r\n                                    <td>:</td>\r\n                                    <td>\r\n                                        <span class=\"badge badge-pill badge-success\" style=\"font-size: 14px;border-radius: .3rem;padding: .25em .5em .2em;\">\r\n                                            {{ product.weight }} gram(s)\r\n                                        </span>\r\n                                    </td>\r\n                                </tr>\r\n                            </tbody>\r\n                        </table>\r\n                        <button class=\"btn btn-primary btn-lg btn-block\">\r\n                            <i class=\"fa fa-shopping-cart\"></i>\r\n                            ADD TO CART\r\n                        </button>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n        <div class=\"row mt-5\">\r\n            <div class=\"col-md-12\">\r\n                <div class=\"card border-0 rounded shadow\">\r\n                    <div class=\"card-body\">\r\n                        <label class=\"font-weight-bold\" style=\"font-size: 20px;\">\r\n                            DESCRIPTION\r\n                        </label>\r\n                        <hr>\r\n                        <div v-html=\"product.content\"></div>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n\r\n    import{ computed, onMounted } from 'vue'\r\n    import { useStore } from 'vuex'\r\n    import { useRoute, useRouter } from 'vue-router'\r\n\r\n    export default {\r\n        name: 'ShowProductComponent',\r\n\r\n        setup() {\r\n\r\n            // Vue route\r\n            const route = useRoute()\r\n\r\n            // Vue router\r\n            const router = useRouter()\r\n\r\n            // Store vuex\r\n            const store = useStore()\r\n\r\n            // onMounted akan menjalankan action \"getDetailProduct\" di module \"product\" Vuex\r\n            onMounted(() => {\r\n                store.dispatch('product')\r\n            })\r\n        }\r\n    }\r\n\r\n</script>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}