{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { ref, reactive } from 'vue';\nimport { useStore } from 'vuex';\nimport { useRouter } from 'vue-router';\nexport default {\n  name: 'LoginComponent',\n  setup() {\n    // User state\n    const user = reactive({\n      email: '',\n      password: ''\n    });\n\n    // Validation state\n    const validation = ref([]);\n\n    // Store vuex\n    const store = useStore();\n\n    // Route\n    const router = useRouter();\n\n    // Method login\n    function login() {\n      // Define variable\n      let email = user.email;\n      let password = user.password;\n\n      // Panggil action \"login\" dari module \"auth\" di vuex\n      store.dispatch('auth/login', {\n        email,\n        password\n      }).then(() => {\n        // Redirect to dashboard\n        router.push({\n          name: 'dashboard'\n        });\n      }).catch;\n    }\n  }\n};","map":{"version":3,"mappings":";AA8CI,SAASA,GAAG,EAAEC,QAAO,QAAS,KAAI;AAClC,SAASC,QAAO,QAAS,MAAK;AAC9B,SAASC,SAAQ,QAAS,YAAW;AAErC,eAAe;EACXC,IAAI,EAAE,gBAAgB;EAEtBC,KAAK,GAAG;IAEJ;IACA,MAAMC,IAAG,GAAIL,QAAQ,CAAC;MAClBM,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE;IACd,CAAC;;IAED;IACA,MAAMC,UAAS,GAAIT,GAAG,CAAC,EAAE;;IAEzB;IACA,MAAMU,KAAI,GAAIR,QAAQ,EAAC;;IAEvB;IACA,MAAMS,MAAK,GAAIR,SAAS,EAAC;;IAEzB;IACA,SAASS,KAAK,GAAG;MAEb;MACA,IAAIL,KAAI,GAAID,IAAI,CAACC,KAAI;MACrB,IAAIC,QAAO,GAAIF,IAAI,CAACE,QAAO;;MAE3B;MACAE,KAAK,CAACG,QAAQ,CAAC,YAAY,EAAE;QACzBN,KAAK;QACLC;MACJ,CAAC,EACAM,IAAI,CAAC,MAAM;QAER;QACAH,MAAM,CAACI,IAAI,CAAC;UAACX,IAAI,EAAE;QAAW,CAAC;MACnC,CAAC,EACAY,KAAI;IACT;EACJ;AACJ","names":["ref","reactive","useStore","useRouter","name","setup","user","email","password","validation","store","router","login","dispatch","then","push","catch"],"sourceRoot":"","sources":["D:\\xampp\\htdocs\\larastore\\frontend-larastore\\src\\views\\auth\\Login.vue"],"sourcesContent":["<template>\r\n    <div class=\"container-fluid mb-5 mt-5\">\r\n        <div class=\"row justify-content-center\">\r\n            <div class=\"col-md-4\">\r\n                <div v-if=\"validation.message\" class=\"mt-2 alert alert-danger\">\r\n                    {{ validation.message }}\r\n                </div>\r\n                <div class=\"card border-0 shadow rounded\">\r\n                    <div class=\"card-body\">\r\n                        <h4>LOGIN</h4>\r\n                        <hr>\r\n                        <form @submit-prevent=\"login\">\r\n                            <div class=\"form-group\">\r\n                                <label>Email Address</label>\r\n                                <input type=\"email\" v-model=\"user.email\" class=\"form-control\" placeholder=\"Email Address\">\r\n                            </div>\r\n                            <div v-if=\"validation.email\" class=\"mt-2 alert alert-danger\">\r\n                                {{ validation.email[0] }}\r\n                            </div>\r\n                            <div class=\"form-group\">\r\n                                <label>Password</label>\r\n                                <input type=\"password\" v-modal=\"user.password\" class=\"form-control\" placeholder=\"Enter password\">\r\n                            </div>\r\n                            <div v-if=\"validation.password\" class=\"alert alert-danger\">\r\n                                {{ validation.passwords[0] }}\r\n                            </div>\r\n                            <div class=\"form-group form-check\">\r\n                                <input type=\"checkbox\" class=\"form-check-input\" id=\"exampleCheck1\">\r\n                                <label for=\"exampleCheck1\" class=\"form-check-label\">Remember me</label>\r\n                            </div>\r\n                            <button type=\"submit\" class=\"btn btn-primary btn-block\">LOGIN</button>\r\n                        </form>\r\n                    </div>\r\n                </div>\r\n                <div class=\"re