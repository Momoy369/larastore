{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\n// Import customer menu component\nimport CustomerMenu from '@/components/CustomerMenu';\nimport { computed, onMounted } from 'vue';\nimport { useStore } from 'vuex';\nimport { useRoute, useRouter } from 'vue-router';\nexport default {\n  name: 'OrderShowComponent',\n  components: {\n    // Customer menu component\n    CustomerMenu\n  },\n  setup() {\n    // Store Vuex\n    const store = useStore();\n\n    // Router and route Vue Router\n    const route = useRoute();\n    const router = useRouter();\n\n    // Mounted\n    onMounted(() => {\n      // Panggil action \"detailOrder\" di dalam module \"order\" di Vuex\n      store.dispatch('order/detailOrder', route.params.snap_token);\n    });\n\n    // Computed\n    const detailOrder = computed(() => {\n      // Panggil getter dengan nama \"detailOrder\" di dalam module \"order\" Vuex\n      return store.getters('order/detailOrder');\n    });\n\n    // Computed\n    const productInOrder = computed(() => {\n      // Panggil getter dengan nama \"productInOrder\" di dalam module \"order\" Vuex\n      return store.getters('order/productInOrder');\n    });\n\n    // Function payment \"Midtrans\"\n    function payment(snap_token) {\n      window.snap.pay(snap_token, {\n        onSuccess: function () {\n          router.push({\n            name: 'detail_order',\n            params: {\n              snap_token: snap_token\n            }\n          });\n        },\n        onPending: function () {\n          router.push({\n            name: 'detail_order',\n            params: {\n              snap_token: snap_token\n            }\n          });\n        },\n        onError: function () {\n          router.push({\n            name: 'detail_order',\n            params: {\n              snap_token: snap_token\n            }\n          });\n        }\n      });\n    }\n    return {\n      store,\n      route,\n      router,\n      detailOrder,\n      productInOrder,\n      payment\n    };\n  }\n};","map":{"version":3,"mappings":";AAyHA;AACA,OAAOA,YAAW,MAAO,2BAA0B;AACnD,SAASC,QAAQ,EAAEC,SAAQ,QAAS,KAAI;AACxC,SAASC,QAAO,QAAS,MAAK;AAC9B,SAASC,QAAQ,EAAEC,SAAQ,QAAS,YAAW;AAE/C,eAAe;EAEXC,IAAI,EAAE,oBAAoB;EAE1BC,UAAU,EAAE;IAER;IACAP;EACJ,CAAC;EAEDQ,KAAK,GAAG;IAEJ;IACA,MAAMC,KAAI,GAAIN,QAAQ,EAAC;;IAEvB;IACA,MAAMO,KAAI,GAAIN,QAAQ,EAAC;IACvB,MAAMO,MAAK,GAAIN,SAAS,EAAC;;IAEzB;IACAH,SAAS,CAAC,MAAM;MAEZ;MACAO,KAAK,CAACG,QAAQ,CAAC,mBAAmB,EAAEF,KAAK,CAACG,MAAM,CAACC,UAAU;IAC/D,CAAC;;IAED;IACA,MAAMC,WAAU,GAAId,QAAQ,CAAC,MAAM;MAE/B;MACA,OAAOQ,KAAK,CAACO,OAAO,CAAC,mBAAmB;IAC5C,CAAC;;IAED;IACA,MAAMC,cAAa,GAAIhB,QAAQ,CAAC,MAAM;MAElC;MACA,OAAOQ,KAAK,CAACO,OAAO,CAAC,sBAAsB;IAC/C,CAAC;;IAED;IACA,SAASE,OAAO,CAACJ,UAAU,EAAE;MAEzBK,MAAM,CAACC,IAAI,CAACC,GAAG,CAACP,UAAU,EAAE;QAExBQ,SAAS,EAAE,YAAY;UACnBX,MAAM,CAACY,IAAI,CAAC;YAACjB,IAAI,EAAE,cAAc;YAAEO,MAAM,EAAC;cAACC,UAAU,EAAEA;YAAU;UAAC,CAAC;QACvE,CAAC;QAEDU,SAAS,EAAE,YAAY;UACnBb,MAAM,CAACY,IAAI,CAAC;YAACjB,IAAI,EAAE,cAAc;YAAEO,MAAM,EAAC;cAACC,UAAU,EAAEA;YAAU;UAAC,CAAC;QACvE,CAAC;QAEDW,OAAO,EAAE,YAAY;UACjBd,MAAM,CAACY,IAAI,CAAC;YAACjB,IAAI,EAAE,cAAc;YAAEO,MAAM,EAAC;cAACC,UAAU,EAAEA;YAAU;UAAC,CAAC;QACvE;MACJ,CAAC;IACL;IAEA,OAAO;MACHL,KAAK;MACLC,KAAK;MACLC,MAAM;MACNI,WAAW;MACXE,cAAc;MACdC;IACJ;EACJ;AAEJ","names":["CustomerMenu","computed","onMounted","useStore","useRoute","useRouter","name","components","setup","store","route","router","dispatch","params","snap_token","detailOrder","getters","productInOrder","payment","window","snap","pay","onSuccess","push","onPending","onError"],"sourceRoot":"","sources":["D:\\xampp\\htdocs\\larastore\\frontend-larastore\\src\\views\\order\\Show.vue"],"sourcesContent":["<template>\r\n    <div class=\"continer-fluid mb-5 mt-4\">\r\n        <div class=\"row\">\r\n            <div class=\"col-md-3 mb-4\">\r\n                <CustomerMenu />\r\n            </div>\r\n            <div class=\"col-md-9 mb-4\">\r\n                <div class=\"card border-0 rounded shadow\">\r\n                    <div class=\"card-body\">\r\n                        <h5 class=\"font-weight-bold\">\r\n                            <i class=\"fas fa-shopping-cart\"></i>\r\n                            ORDER DETAILS\r\n                        </h5>\r\n                        <hr>\r\n                        <table class=\"table table-bordered\">\r\n                            <tr>\r\n                                <td style=\"width: 25%;\">NO. INVOICE</td>\r\n                                <td style=\"width: 1%;\">: </td>\r\n                                <td>\r\n                                    {{ detailOrder.invoice }}\r\n                                </td>\r\n                            </tr>\r\n                            <tr>\r\n                                <td>FULLNAME</td>\r\n                                <td>:</td>\r\n                                <td>\r\n                                    {{ detailOrder.name }}\r\n                                </td>\r\n                            </tr>\r\n                            <tr>\r\n                                <td>PHONE/WHATSAPP</td>\r\n                            <td>:</td>\r\n                            <td>{{ detailOrder.phone }}</td>\r\n                            </tr>\r\n                            <tr>\r\n                                <td>COURIER / SERVICE / COST</td>\r\n                                <td>:</td>\r\n                                <td>\r\n                                    {{ detailOrder.courier }} / {{ detailOrder.service }} / R. {{ detailOrder.cost_courier }}\r\n                                </td>\r\n                            </tr>\r\n                            <tr>\r\n                                <td>ADDRESS</td>\r\n                                <td>:</td>\r\n                                <td>\r\n                                    {{ detailOrder.address }}\r\n                                </td>\r\n                            </tr>\r\n                            <tr>\r\n                                <td>GRAND TOTAL</td>\r\n                                <td>:</td>\r\n                                <td>Rp. {{ detailOrder.grand_total }}</td>\r\n                            </tr>\r\n                            <tr>\r\n                                <td>STATUS</td>\r\n                                <td>:</td>\r\n                                <td>\r\n                                    <button @click=\"payment(detailOrder.snap_token)\" v-if=\"detailOrder.status == 'pending'\" class=\"btn btn-primary\">\r\n                                        PAY NOW\r\n                                    </button>\r\n                                    <button v-else-if=\"detailOrder.status == 'success'\" class=\"btn btn-success\">\r\n                                        {{ detailOrder.status }}\r\n                                    </button>\r\n                                    <button v-else-if=\"detailOrder.status == 'expired'\" class=\"btn btn-warning\">\r\n                                        {{ detailOrder.status }}\r\n                                    </button>\r\n                                    <button v-else-if=\"detailOrder.status == 'failed'\" class=\"btn btn-danger\">\r\n                                        {{ detailOrder.status }}\r\n                                    </button>\r\n                                </td>\r\n                            </tr>\r\n                        </table>\r\n                    </div>\r\n                </div>\r\n                \r\n                <div class=\"card border-0 rounded shadow mt-4\">\r\n                    <div class=\"card-body\">\r\n                        <h5>\r\n                            <i class=\"fa fa-shopping-cart\"></i>\r\n                            ORDER DETAILS\r\n                        </h5>\r\n                        <hr>\r\n                        <table class=\"table\" style=\"border-style: solid !important; border-color: rgb(198, 206, 214) !important;\">\r\n                        \r\n                            <tbody>\r\n                                <tr v-for=\"product in productInOrder\" :key=\"product.id\" style=\"background: #edf2f7;\">\r\n                                    <td class=\"b-none\" width=\"25%\">\r\n                                        <div class=\"wrapper-image-cart\">\r\n                                            <img :src=\"product.image\" style=\"width: 100%; border-radius: .5rem;\">\r\n                                        </div>\r\n                                    </td>\r\n                                    <td class=\"b-none\" width=\"50%\">\r\n                                        <h5>\r\n                                            <b>{{ product.product_name }}</b>\r\n                                        </h5>\r\n                                        <table class=\"table-borderless\" style=\"font-size: 14px;\">\r\n                                            <tr>\r\n                                                <td style=\"padding: .20rem;\">QTY</td>\r\n                                                <td style=\"padding: .20rem;\">:</td>\r\n                                                <td style=\"padding: .20rem;\"><b>{{ product.qty }}</b></td>\r\n                                            </tr>\r\n                                        </table>\r\n                                    </td>\r\n                                    <td class=\"b-none text-right\">\r\n                                        <p class=\"m-0 font-weight-bold\">\r\n                                            Rp. {{ moneyFormat(product.price) }}\r\n                                        </p>\r\n                                    </td>\r\n                                </tr>\r\n                            </tbody>\r\n\r\n                        </table>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n\r\n// Import customer menu component\r\nimport CustomerMenu from '@/components/CustomerMenu'\r\nimport { computed, onMounted } from 'vue'\r\nimport { useStore } from 'vuex'\r\nimport { useRoute, useRouter } from 'vue-router'\r\n\r\nexport default {\r\n\r\n    name: 'OrderShowComponent',\r\n\r\n    components: {\r\n\r\n        // Customer menu component\r\n        CustomerMenu\r\n    },\r\n\r\n    setup() {\r\n\r\n        // Store Vuex\r\n        const store = useStore()\r\n\r\n        // Router and route Vue Router\r\n        const route = useRoute()\r\n        const router = useRouter()\r\n\r\n        // Mounted\r\n        onMounted(() => {\r\n\r\n            // Panggil action \"detailOrder\" di dalam module \"order\" di Vuex\r\n            store.dispatch('order/detailOrder', route.params.snap_token)\r\n        })\r\n\r\n        // Computed\r\n        const detailOrder = computed(() => {\r\n\r\n            // Panggil getter dengan nama \"detailOrder\" di dalam module \"order\" Vuex\r\n            return store.getters('order/detailOrder')\r\n        })\r\n\r\n        // Computed\r\n        const productInOrder = computed(() => {\r\n\r\n            // Panggil getter dengan nama \"productInOrder\" di dalam module \"order\" Vuex\r\n            return store.getters('order/productInOrder')\r\n        })\r\n\r\n        // Function payment \"Midtrans\"\r\n        function payment(snap_token) {\r\n\r\n            window.snap.pay(snap_token, {\r\n\r\n                onSuccess: function () {\r\n                    router.push({name: 'detail_order', params:{snap_token: snap_token}})\r\n                },\r\n\r\n                onPending: function () {\r\n                    router.push({name: 'detail_order', params:{snap_token: snap_token}})\r\n                },\r\n\r\n                onError: function () {\r\n                    router.push({name: 'detail_order', params:{snap_token: snap_token}})\r\n                },\r\n            })\r\n        }\r\n\r\n        return {\r\n            store,\r\n            route,\r\n            router,\r\n            detailOrder,\r\n            productInOrder,\r\n            payment\r\n        }\r\n    }\r\n    \r\n}\r\n\r\n</script>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}